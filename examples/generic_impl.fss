trait Test {
    let test: (self, unit: Unit) -> Unit
}

trait AutoTest {
    let auto: (self, unit: Unit, unit: Unit) -> Unit
}

impl Test for String {
    let test = { self, unit -> unit }
}

impl<T: Test> AutoTest for T {
    let auto = { self, _, _ -> self.test(()) }
}

let t = "string"
t.auto()
